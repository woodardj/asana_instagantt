{
  "name": "Open Instagantt from Asana",
  "version": "1.0.0.1",
	"manifest_version": 2,
  "description": "Add a link to Instagantt from the Asana project dashboard.",
  "content_scripts": [{
      "matches": ["*://app.asana.com/*"],
      "js": ["jquery.min.js", "content_script.js"]
  }],
  "icons" : {
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "web_accessible_resources":["img/*"]
}
